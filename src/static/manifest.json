{
  "manifest_version": 3,
  "name": "Web3 Wizard Helper",
  "description": "Extension to simplify querying and data hacking on Dune Analytics",
  "options_page": "options.html",
  "version": "0.0.6",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-32.png"
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://dune.com/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "css": [
        "injected.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon-128.png",
        "icon-32.png",
        "injectedscript.js",
        "icons/*.svg"
      ],
      "matches": [
        "https://dune.com/*"
      ]
    }
  ]
}